<+>import pyautogui\r\nfrom PIL import Image\r\nimport time\r\n\r\n# A dictionary to store Minesweeper board corners\r\n# values 200% zoom of minesweeperonline.com in firefox window\r\nboard_coords = {\r\n    \"beginner\": {\r\n        \"top_left\": (492, 248),   # (x, y)\r\n        \"bottom_right\": (780, 536),\r\n        \"rows_column\": (9, 9)\r\n    },\r\n    \"intermediate\": {\r\n        \"top_left\": (492, 248),\r\n        \"bottom_right\": (1004, 760),\r\n        \"rows_column\": (16, 16)\r\n    },\r\n    \"advanced\": {\r\n        \"top_left\": (472, 248),\r\n        \"bottom_right\": (1432, 760),\r\n        \"rows_column\": (16, 30)\r\n    }\r\n}\r\ndifficulty = \"advanced\"\r\ntile_size = 32\r\n\r\n#delay start\r\ntimer = 5\r\nfor i in range(timer,0,-1):\r\n    print(f'{i}...')\r\n    time.sleep(1)\r\n\r\n#take screenshot\r\ntop_left_x, top_left_y = board_coords[difficulty][\"top_left\"]\r\nbottom_right_x, bottom_right_y = board_coords[difficulty][\"bottom_right\"]\r\n\r\n\r\nwidth = bottom_right_x - top_left_x\r\nheight = bottom_right_y - top_left_y\r\n\r\ngame_region = (top_left_x, top_left_y, width, height)\r\nimage = pyautogui.screenshot(region=game_region)\r\n\r\n\r\n#define grid size\r\nrows, columns = board_coords[difficulty][\"rows_column\"]\r\n\r\ngrid = []\r\nfor row in range (rows):\r\n    grid_row = []\r\n    for column in range (columns):\r\n        #the pixel to consider\r\n        x = column * tile_size + (tile_size -14)\r\n        y = row * tile_size + (tile_size - 26)\r\n\r\n        pixel = image.getpixel((x, y))\r\n\r\n        if pixel == (189, 189, 189):\r\n            #check if it is opened or unopened\r\n            corner_pixel = image.getpixel((x - 16, y - 0))\r\n            if corner_pixel == (255, 255, 255):\r\n                value = '-'\r\n            else:\r\n                value = '0'\r\n\r\n        #assign values to colors\r\n        elif pixel == (0, 0, 255):\r\n            value = '1'\r\n        elif pixel == (0, 123, 0):\r\n            value = '2'\r\n        elif pixel == (255, 0, 0):\r\n            value = '3'\r\n        elif pixel == (0, 0, 123):\r\n            value = '4'\r\n\r\n\r\n\r\n\r\n        else:\r\n            value = 'ND'\r\n\r\n        #make final grid\r\n        grid_row.append(value)\r\n    grid.append(grid_row)\r\n\r\n\r\n#print the grid\r\nfor row in grid:\r\n    print(\" \".join(row))\r\n\r\n\r\n\r\n
